{
  "name": "feely",
  "version": "0.0.1",
  "description": "User feedbacks get real by being rewarded",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env next dev -p 3000",
    "build": "env-cmd -f .env next build",
    "start": "env-cmd -f .env next start -p 3000",
    "lint": "next lint",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "stripe-listener": "stripe listen --forward-to localhost:3000/api/webhook"
  },
  "dependencies": {
    "@fucina/tailwind": "0.0.7",
    "@fucina/ui": "0.0.17",
    "@fucina/utils": "^0.0.5",
    "@prisma/client": "^5.18.0",
    "@react-three/drei": "^9.106.0",
    "@react-three/fiber": "^8.16.6",
    "@stripe/stripe-js": "^4.8.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.43.4",
    "@vercel/analytics": "^1.3.1",
    "axios": "^1.7.2",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "env-cmd": "^10.1.0",
    "geist": "^1.3.1",
    "next": "14.2.7",
    "next-themes": "^0.3.0",
    "prisma": "^5.16.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "react-query": "^3.39.3",
    "stripe": "^17.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20",
    "@types/react": "18.2.42",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5"
  }
